@model Queue

<form class="form-signin" method="get" asp-action="Get" asp-controller="Queue">
    <table class="table table-dark shadow mt-5 mb-3 border border-5 border-light rounded-3">
        <thead>
            <tr>
                <th scope="col">Queue Number</th>
                <th scope="col">Fullname</th>
                <th scope="col">PhoneNumber</th>
                <th scope="col">Time taken queue</th>
                <th scope="col">Time will come</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="col">@Model.Number</th>
                <th scope="col">@Model.Fullname</th>
                <th scope="col">(XX) XXX XX @Model.PhoneNumber.Substring(Model.PhoneNumber.Length - 2)</th>
                <th scope="col">@Model.CreatedAt.ToString("MMM dd, yyyy HH:mm")</th>
                <th scope="col">@Model.ServiceTime.ToString("MMM dd, yyyy HH:mm")</th>
            </tr>
        </tbody>
    </table>
</form>
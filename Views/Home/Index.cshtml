@model List<Queue>

<table class="table table-dark shadow mt-5 mb-3 border border-5 border-light rounded-3">
    <thead>
        <tr>
            <th scope="col">Queue Number</th>
            <th scope="col">Fullname</th>
            <th scope="col">Phone Number</th>
            <th scope="col">Time taken queue</th>
            <th scope="col">Time will come</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var queue in Model)
        {
            @if (DateTimeOffset.UtcNow < queue.ServiceTime.AddMinutes(5))
            {
                <tr>
                    <td>@queue.Number</td>
                    <td>@queue.Fullname</td>
                    <td>(XX) XXX XX @queue.PhoneNumber.Substring(queue.PhoneNumber.Length - 2)</td>
                    <td>@queue.CreatedAt.ToString("MMM dd, yyyy HH:mm")</td>
                    <td>@queue.ServiceTime.ToString("MMM dd, yyyy HH:mm")</td>
                </tr>
            }
        }

    </tbody>
</table>
